{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "Auth": "./src/Auth",
        "Explore": "./src/Explore",
        "Gife": "./src/Gife",
        "Global": "./src/Global",
        "Profile": "./src/Profile"
      }
    }]
  ]
}

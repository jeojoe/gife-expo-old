{
  "presets": ["babel-preset-expo"],
  "env": {
    "development": {
      "plugins": ["transform-react-jsx-source"]
    }
  },
  "plugins": [
    ["module-resolver", {
      "root": ["./src"],
      "alias": {
        "modules": "./src/modules",
        "hoc": "./src/hoc",
        "app-constants": "./src/constants",
        "assets": "./src/assets",
        "navigators": "./src/navigators"
      }
    }]
  ]
}
